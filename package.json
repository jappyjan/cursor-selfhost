{
  "name": "cursor-selfhost",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:api & pnpm run dev:web",
    "dev:api": "pnpm --filter api dev",
    "dev:web": "pnpm --filter web dev",
    "build": "pnpm -r run build",
    "db:migrate": "pnpm --filter db migrate",
    "db:generate": "pnpm --filter db generate",
    "test": "pnpm run test:api && pnpm --filter db test",
    "test:api": "pnpm --filter api test",
    "test:e2e": "RUN_E2E_WITH_REAL_CURSOR=1 pnpm --filter api test",
    "start": "pnpm run start:api & pnpm run start:web",
    "start:api": "pnpm --filter api start",
    "start:web": "pnpm --filter web build && pnpm --filter web preview",
    "stop": "./scripts/stop.sh",
    "restart": "./scripts/restart.sh",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.14.2"
}
