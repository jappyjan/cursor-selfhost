# API environment variables
# Copy to .env and adjust

# Port for the API server (default: 3001)
PORT=3001

# Root path for all projects; required for first-run; can be set via UI
PROJECTS_BASE_PATH=

# SQLite database path (default: ./data/cursor-selfhost.sqlite)
DATABASE_PATH=

# Cursor CLI path (default: cursor)
CURSOR_CLI_PATH=cursor

# Cursor API key (optional; or use `cursor agent login`)
# CURSOR_API_KEY=
