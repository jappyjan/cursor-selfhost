{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm exec tsx watch main.ts",
    "start": "pnpm exec tsx main.ts",
    "test": "vitest run",
    "dump:cli": "node scripts/dump-cursor-cli-output.js",
    "test:cli": "CURSOR_CLI_PATH=agent node scripts/test-cli-input-formats.js",
    "test:cli:auth": "CURSOR_CLI_PATH=agent node scripts/test-cli-input-formats.js --auth-check-only",
    "test:cli:vitest": "RUN_CLI_FORMAT_TESTS=1 CURSOR_CLI_PATH=agent vitest run cli-input-formats",
    "test:e2e": "RUN_E2E_WITH_REAL_CURSOR=1 vitest run",
    "test:e2e:dump": "RUN_E2E_WITH_REAL_CURSOR=1 DUMP_E2E_TOOL_CALLS=1 vitest run integration.test"
  },
  "dependencies": {
    "@cursor-selfhost/db": "workspace:*",
    "@hono/node-server": "^1.13.0",
    "hono": "^4.6.0",
    "nanoid": "^5.0.9",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "dotenv": "^17.3.1",
    "tsx": "^4.19.2",
    "vitest": "^4.0.18"
  }
}
